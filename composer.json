{
    "name": "bit3/contao-theme-reveal-js-basic",
    "type": "contao-module",
    "description": "reveal-js basic template for the Contao Open Source CMS",
    "keywords": [
        "contao",
        "theme",
        "reveal-js"
    ],
    "license": "LGPLv3",
    "authors": [
        {
            "name": "Tristan Lins",
            "email": "tristan.lins@bit3.de",
            "homepage": "http://bit3.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.4",
        "contao/core": "~3.3",
        "contao-community-alliance/composer-plugin": "~2.0",
        "bit3/contao-meta-palettes": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\Contao\\Theme\\RevealJs\\Basic\\": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "module": "system/modules/reveal-js",
                "assets/reveal-js": "assets/reveal-js",
                "assets/theme": "assets/reveal-js-theme"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}
